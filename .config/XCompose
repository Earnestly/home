<Multi_key> <m> <n> : "ğ’"
<Multi_key> <m> <l> : "Î»"
<Multi_key> <m> <p> : "Ï€"
<Multi_key> <m> <d> : "Î”"
<Multi_key> <m> <e> : "Ïµ"
<Multi_key> <m> <i> : "âˆ"
<Multi_key> <m> <x> : "ğ‘¿"
<Multi_key> <m> <y> : "ğ’€"

# quotes
<Multi_key> <bracketleft> <apostrophe> : "ï½¢"
<Multi_key> <bracketright> <apostrophe> : "ï½£"

# misc
<Multi_key> <colon> <bar> <bar> : "à² _à² "
<Multi_key> <less> <3> : "â¤"

# fun with paleotypography
<Multi_key> <T> <H> : "Ã" # 'Th'e
<Multi_key> <t> <h> : "Ã°" 

<Multi_key> <W> <W> : "Ç·" # 'W'hen
<Multi_key> <w> <w> : "Æ¿" 

