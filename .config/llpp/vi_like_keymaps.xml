<llppconfig>
    <defaults

    <keymap mode='global'>
        <map in='alt-i' out='l'/>
        <map in='alt-r' out='u'/>
    </keymap>
    <keymap mode='view'>
        <map in='k' out='up'/>
        <map in='ctrl-u' out='ctrl-k'/>
        <map in='u' out='ctrl-k'/>
        <map in='l' out='right'/>
        <map in='j' out='down'/>
        <map in='f' out='pgdown'/>
        <map in='d' out='ctrl-j'/>
        <map in='ctrl-d' out='ctrl-j'/>
        <map in='b' out='pgup'/>
        <map in='h' out='left'/>
    </keymap>

</defaults>

</llppconfig>
